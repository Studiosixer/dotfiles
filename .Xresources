! ------------------------------------------------------------------------------
! Cursor selection stuff from here: https://github.com/muennich/urxvt-perls
URxvt.perl-lib: /home/waeledui/Dev/urxvt-perls/
! ------------------------------------------------------------------------------

Xcursor.size: 16W
URxvt.urgentOnBell: true
URxvt.scrollBar: false
URxvt.font: xft:DejaVu Sans Mono Nerd Font:size=12, xft:Symbola
URxvt.depth: 32
URxvt.url-launcher: /usr/bin/chromium
URxvt.matcher.button: 1
URxvt.perl-ext-common: default,matcher,vtwheel,font-size,keyboard-select
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.keysym.M-s: perl:keyboard-select:search

! ------------------------------------------------------------------------------
! ROFI Color theme
! ------------------------------------------------------------------------------
rofi.color-enabled: true
rofi.color-window: #273238, #273238, #1e2529
rofi.color-normal: #273238, #c1c1c1, #273238, #394249, #ffffff
rofi.color-active: #273238, #80cbc4, #273238, #394249, #80cbc4
rofi.color-urgent: #273238, #ff1844, #273238, #394249, #ff1844
